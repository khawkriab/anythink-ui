{
  "name": "anythink-ui",
  "version": "0.0.9",
  "packageManager": "^npm@9.0.0",
  "description": "UI",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:publish": "npm run build & npm publish --access=public",
    "clean": "rimraf ./dist && rimraf ./%npm_package_name%-%npm_package_version%.tgz",
    "dev": "npm run build && npm pack && cd example && npm i --save ../%npm_package_name%-%npm_package_version%.tgz && cross-env FAST_REFRESH=false react-scripts start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khawkriab/anythink-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "khawkriab sittisak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khawkriab/anythink-ui/issues"
  },
  "homepage": "https://github.com/khawkriab/anythink-ui.git#readme",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "cross-env": "^7.0.3",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist"
  ]
}
